<div class="agenda">
  {{#each this}}
    <section class="agenda">
      <div class="meta">
        {{user.username}}
        on
        {{format_date created_at}}
      </div>
      <div class="text">
        {{agenda_text}}
      </div>
      <div>
        <button id="update-agenda-btn" type="submit">Edit agenda</button>
      </div>
    </section>
  {{/each}}
</div>