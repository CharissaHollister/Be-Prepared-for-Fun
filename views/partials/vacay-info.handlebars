<article class="vacay">
  <div class="title">
  <a id={{id}} href="/calendar/{{id}}" class="calendar">{{title}}</a>
    <span>({{format_date start_date}} to {{format_date end_date}})</span>
  </div>
  <div class="meta">
    Destination:
    {{destination}}
    | created by:
    {{user.username}}

    <div># of Events: {{AgendaItem.length}}</div>
    {{! todo ^ count not working  }}
  </div>
</article>